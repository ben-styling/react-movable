{
  "name": "react-movable",
  "version": "3.2.0",
  "type": "module",
  "description": "Drag and drop lists.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tajo/react-movable.git"
  },
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && tsc --outDir lib && cp flowtypes/* lib && rm -rf lib/examples && mv lib/src/* lib && rm -rf lib/src && rm lib/*.test.* && rm lib/*.stories.*",
    "jest:e2e": "jest --runInBand",
    "jest:e2e:update": "jest --runInBand -u e2e",
    "ladle:ci": "ladle serve",
    "test": "yarn typecheck && yarn test:e2e && yarn test:unit",
    "test:e2e": "start-server-and-test ladle:ci http-get://localhost:61000 jest:e2e",
    "test:e2e:dev": "E2E=dev yarn jest:e2e",
    "test:e2e:update": "start-server-and-test ladle:ci http-get://localhost:61000 jest:e2e:update",
    "test:unit": "jest src",
    "typecheck": "tsc --noEmit"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-dom",
    "react-component",
    "sortable",
    "reorder",
    "reorderable",
    "drag",
    "drop",
    "list",
    "table",
    "drag and drop",
    "dnd",
    "movable"
  ],
  "devDependencies": {
    "@ladle/react": "^4.1.0",
    "@types/expect-puppeteer": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jest-environment-puppeteer": "5.0.6",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "^20.14.8",
    "@types/react": "~18.3.3",
    "@types/react-dom": "~18.3.0",
    "imgur": "^2.4.2",
    "jest": "^29.7.0",
    "jest-image-snapshot": "6.4.0",
    "jest-puppeteer": "^10.0.1",
    "puppeteer": "^22.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "start-server-and-test": "^2.0.4",
    "ts-jest": "^29.1.5",
    "typescript": "5.5.2"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/e2e/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "reporters": [
      "default",
      "<rootDir>/image-reporter.cjs"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-puppeteer"
  },
  "volta": {
    "node": "20.14.0",
    "yarn": "1.22.17"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
